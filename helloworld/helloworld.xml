<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <TemplateURL>https://raw.githubusercontent.com/nthDerivative/docker/master/helloworld/helloworld.xml</TemplateURL>
  <Beta>False</Beta>
  <Category>Productivity: Tools:</Category>
  <Name>helloworld</Name>
  <Description>
    Test
  </Description>
  <Overview>
    Test
  </Overview>
  <Support></Support>
  <Registry></Registry>
  <GitHub>https://github.com/nthDerivative/docker/tree/master/helloworld</GitHub>
  <Repository>nthDerivaitve/docker</Repository>
  <Project>https://hirosky.com</Project>
  <BindTime>true</BindTime>
  <Privileged>true</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>5000</HostPort>
        <ContainerPort>5000</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name>UMASK</Name>
      <Value>000</Value>
    </Variable>
    <Variable>
      <Name>PUID</Name>
      <Value>99</Value>
    </Variable>
    <Variable>
      <Name>PGID</Name>
      <Value>100</Value>
    </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/appdata/config</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user</HostDir>
      <ContainerDir>/media</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:6080]/vnc.html?resize=remote&amp;host=[IP]&amp;port=[PORT:6080]&amp;autoconnect=1</WebUI>
  <Banner></Banner>
  <Icon></Icon>
  <DonateText>Hello!</DonateText>
  <DonateLink></DonateLink>
  <DonateImg></DonateImg>
  <ExtraParams></ExtraParams>
</Containers>