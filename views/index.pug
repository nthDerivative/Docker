extends default

block content
  div.container
    each project in projects
      div.project
        div.project-image(style=`background: url('/images/${project.imgSrc}') top center
        no-repeat; background-size: cover;`)
        h2.projectname-name
          | #{project.name} }
        a(href=`/profile?id=${project.id}`)
          | View Profile
